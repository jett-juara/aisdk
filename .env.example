# Local environment (DO NOT COMMIT REAL SECRETS)
# Isi nilai variabel sesuai kebutuhan. File ini tidak untuk dibagikan.

# GitHub
GITHUB_FINE_GRAINED_TOKEN=github_pat_YOUR_FINE_GRAINED_TOKEN_HERE
GITHUB_CLASSIC_TOKEN=ghp_YOUR_CLASSIC_TOKEN_HERE
GITHUB_TOKEN=ghp_YOUR_GITHUB_TOKEN_HERE
GITHUB_REPOSITORY_URL=https://github.com/OWNER/REPO
GITHUB_OWNER=YOUR_GITHUB_OWNER
GITHUB_REPO=YOUR_REPO_NAME

# Supabase
SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co
SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...YOUR_ANON_KEY_HERE
SUPABASE_SERVICE_ROLE=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...YOUR_SERVICE_ROLE_HERE
SUPABASE_JWT_SECRET=YOUR_JWT_SECRET_HERE
SUPABASE_ACCESS_TOKEN=sbp_YOUR_SUPABASE_ACCESS_TOKEN_HERE

# OpenRouter (Primary AI Model)
OPENROUTER_API_KEY=sk-or-v1-YOUR_OPENROUTER_API_KEY_HERE
OPENROUTER_MODEL=google/gemini-2.5-flash

# Vercel (opsional)
VERCEL_TOKEN=YOUR_VERCEL_TOKEN_HERE

# Feature Flags
STREAM_TIMEOUT_MS=60000
GENERATE_TIMEOUT_MS=60000
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=60
WEB_SEARCH_ENABLE=true

# Public Variables (Client-side)
NEXT_PUBLIC_SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...YOUR_ANON_KEY_HERE
NEXT_PUBLIC_APP_URL=http://localhost:3000
